
<link rel="stylesheet" type="text/css" href="../stylesheets/style1.css">

{{> messages }}
{{#if email}}
<div class="container1">

<h2>{{ first }}'s profile</h2>
<table class="table">
  <tr>
    <td>First Name:</td>
    <td>{{ first }}</td>
  </tr>
  <tr>
    <td>Last Name</td>
    <td>{{ last }}</td>
  </tr>
  <tr>
    <td>Email</td>
    <td>{{ email }}</td>
  </tr>
  {{!-- <tr>
    <td>Facebook:</td>
    <td>{{ facebook }}</td>
  </tr> --}}
</table>
<form action="/auth/logout" method="DELETE">
<button class="">LogOut</button>
</form>
</div>

{{/if}}